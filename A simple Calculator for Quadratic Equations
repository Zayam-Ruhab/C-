#include <iostream>
#include <cmath> // for sqrt

using namespace std;

int main() {
    double a, b, c;
    cout << "Enter a: ";
    cin >> a;
    cout << "Enter b: ";
    cin >> b;
    cout << "Enter c: ";
    cin >> c;

    double d = b*b - 4*a*c; // discriminant

    if (d >= 0) {
        cout << "Root 1: " << (-b + sqrt(d)) / (2*a) << endl;
        cout << "Root 2: " << (-b - sqrt(d)) / (2*a) << endl;
    } else {
        cout << "Root 1: " << -b/(2*a) << " + " << sqrt(-d)/(2*a) << "i" << endl;
        cout << "Root 2: " << -b/(2*a) << " - " << sqrt(-d)/(2*a) << "i" << endl;
    }

    return 0;
}
